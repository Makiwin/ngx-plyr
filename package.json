{
  "name": "ngx-plyr-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:app": "ng build --prod && cp dist/ngx-plyr-app/index.html dist/ngx-plyr-app/404.html",
    "build:lib": "cd projects/ngx-plyr && ng-packagr -p ng-package.json --config tsconfig.ngc.json && cd ../../ && cp README.md dist/ngx-plyr && cp LICENSE.md dist/ngx-plyr",
    "test:app": "ng test ngx-plyr-app",
    "test:app:ci": "ng test ngx-plyr-app --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test:lib": "ng test ngx-plyr",
    "test:lib:ci": "ng test ngx-plyr --watch=false --progress=false --browsers=ChromeHeadlessCI --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "semantic-release",
    "publish": "cd dist/ngx-plyr && npm publish --access public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "core-js": "^2.6.9",
    "dashjs": "^2.9.3",
    "hls.js": "^1.4.12",
    "plyr": "^3.7.8",
    "rxjs": "~6.5.4",
    "tslib": "^2.6.2",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@angular/language-service": "^15.2.10",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@types/jasmine": "^3.3.14",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~5.1.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^1.5.2",
    "ng-packagr": "^15.2.2",
    "protractor": "^7.0.0",
    "semantic-release": "^15.13.18",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.9.5"
  }
}
